        "segment_level_notes_jsonl_path": "path/to/segment_level_notes.jsonl",
        "context_scope_assignments_path": "path/to/context_scope_assignments.json",
        "event_type_assignments_path": "path/to/event_type_assignments.json",
        "dataset_name": null,
        "language_model_provider_config": {
            "provider": "LANGUAGE_MODEL_PROVIDER_AZURE_OPENAI",
