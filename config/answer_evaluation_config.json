{
    "answer_evaluator_config": {
        "generated_answer_jsonl_path": "stitch_output/${DATASET_NAME}/answer_generation/direct_answer_generation.json",
        "questions_jsonl_path": "came_bench_data/decoded/${DATASET_NAME}/questions.jsonl",
        "answer_evaluation_strategy": "answer_evaluation_v1",
        "concurrent": 8,
        "dataset_name": "${DATASET_NAME}",
        "output_dir": "stitch_output/${DATASET_NAME}/answer_evaluation",
        "adapter_language_model_provider_config": {
            "provider": "LANGUAGE_MODEL_PROVIDER_OPENAI",
            "model_name": "openai/qwen-plus",
            "temperature": 1.0,
            "max_tokens": 1024,
            "top_p": 0.9,
            "openai_config": {
                "api_key": "${DASHSCOPE_API_KEY}",
                "api_base": "https://dashscope.aliyuncs.com/compatible-mode/v1"
            }
        }
    },
    "answer_evaluation_strategy_type": "ANSWER_EVALUATION_STRATEGY_TYPE_answer_evaluation_v1",
    "answer_evaluation_v1_config": {
        "language_model_provider_config": {
            "provider": "LANGUAGE_MODEL_PROVIDER_OPENAI",
            "model_name": "openai/qwen-plus",
            "temperature": 1.0,
            "max_tokens": 1024,
            "top_p": 0.9,
            "openai_config": {
                "api_key": "${DASHSCOPE_API_KEY}",
                "api_base": "https://dashscope.aliyuncs.com/compatible-mode/v1"
            }
        }
    }
}
